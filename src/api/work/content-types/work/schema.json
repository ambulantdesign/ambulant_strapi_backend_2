{
  "kind": "collectionType",
  "collectionName": "works",
  "info": {
    "singularName": "work",
    "pluralName": "works",
    "displayName": "Work",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "artist": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::artist.artist"
    },
    "keywords": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::keyword.keyword"
    },
    "smallSlider": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "content": {
      "type": "richtext"
    },
    "meta": {
      "type": "component",
      "repeatable": false,
      "component": "portfolio.work-additional-fields"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo-basic-fields"
    },
    "Gallery": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "Weblink": {
      "type": "component",
      "repeatable": true,
      "component": "link-component.weblink"
    },
    "Videos": {
      "type": "component",
      "repeatable": true,
      "component": "media.video"
    },
    "streamingVideo": {
      "type": "component",
      "repeatable": false,
      "component": "media.streaming-video"
    },
    "productionDate": {
      "type": "date",
      "required": true
    }
  }
}
